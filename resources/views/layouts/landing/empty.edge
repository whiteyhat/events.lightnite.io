    <script>
     function showNotification(type, msg) {
        let icon
        if (type === "success") {
            icon =  "tim-icons icon-check-2"
        } else if (type === "info") {
            icon =  "tim-icons icon-bulb-63"
        } else if (type === "warning") {
            icon = "tim-icons icon-alert-circle-exc"
        } else if (type === "danger") {
            icon = "tim-icons icon-simple-remove"
        }
        $.notify({
          icon,
          message: msg
        }, {
          type: type,
          timer: 8000,
          placement: {
              from: "top",
              align: "center"
          }
      });
      }
    </script>
